{
    "env": {
        "browser": true,
        "commonjs": false,
        "es6": false,
        "node": false,
        "jquery": true,
        "jasmine":false
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": false
        },
        "sourceType": "module"
    },
    "rules": {
        "no-const-assign": "warn",
        "no-this-before-super": "warn",
        "no-undef": "warn",
        "no-unreachable": "warn",
        "no-unused-vars": "off",
        "constructor-super": "warn",
        "valid-typeof": "warn"
    },
    "globals": {
        "IMWS": false,
        "IMatch": false,
        "IMatchTranslate": false,
        "ol":false
    }
}
